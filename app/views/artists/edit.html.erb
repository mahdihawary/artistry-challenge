<%= form_for @artist do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :age %>
<%= f.number_field :age %>
<br>
<%= f.label :instrument %>
<%= f.collection_select :artist, :instrument_id, Instrument.all, :id, :name %>
<br> 
<%= f.label :title %>
<%= f.text_field :title %>
<br>
<%= f.submit %>
<% end %>